class Solution{
    
    // Function to find floor of x
    // arr: input array
    // n is the size of array
    static int findFloor(long arr[], int n, long x)
    {
        int l = 0;
        int h = n -1;
        while(l <= h){
            int mid = l + (h - l)/2;
            
            if(arr[mid] == x)
                return mid;
            else if(arr[mid] < x)
                l = mid + 1;
            else
                h = mid -1;
        }
        return h;
    }
    
}



