        // return -1;
        
  =======================
  
  class Solution{

    static int findFloor(long arr[], int n, long x)
    {
        int i=0;
        int j=n-1;
        int f=-1;
        
        while(i<=j){
            int mid=(i+j)/2;
            if(arr[mid]>x){
                j=mid-1;   
            }else if(arr[mid]<x){        	
                i=mid+1;
                f=mid;
            }else{  
               f=mid;
               break;
            }
        }
        return f;
    }
}
